    "    def __init__(self):\n",
    "        self._name = 'lala' #受保护的属性，只能在类内被调用，在外面无法输出和修改\n",
    "        self.__age = 54 #私有属性，可以输出可以修改但是不能import\n",
    "        self.gender ='男'\n",
    "pe=person()\n",
    "pe._name = 'lala2'\n",
    "print(pe._name)\n",
    "print(pe.gender)\n",
    "#print(pe.__age) #无法输出\n",
